

<div class="wrapper">
<ul class="list">
    <h2>Список сортов чая</h2>
    {{#each teas}}
    <li class="teaName">{{this.name}} <button id='{{this._id}}' class="delete">delete</button></li>
    {{/each}}
</ul>
<form class="signup" class="cabinet">
<h2>Добавить чай</h2>
  <label for="name">Name</label>
  <input type="text" name="name"  />
  <label for="location">Location</label>
  <input type="text" name="location"  />
  <label for="about">About</label>
  <textarea name="text"cols="30" rows="10"></textarea>
  <button>Добавить Чай</button>
</form>
</div>

<script src ="/js/cabinet.js"></script>