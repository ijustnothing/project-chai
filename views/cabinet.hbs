

<div class="wrapper">
<ul class="list">
    <h2>List of teas:</h2>
    {{#each teas}}
    <li id="{{this._id}}" class="teaName"><a class="linkTea" href="{{this._id}}">{{this.name}}</a> <button id='{{this._id}}' class="delete">delete</button></li>
    {{/each}}
</ul>
<form action='/cabinet' method = "post" class="signup" class="cabinet" enctype='multipart/form-data'>
<h2>Add Tea</h2>
  <label for="name">Name</label>
  <input type="text" name="name"  />
  <label for="location">Location</label>
  <input type="text" name="location"  />
  <label for="about">About</label>
  <textarea name="text"cols="30" rows="10"></textarea>
  <label for="text">Img</label>
  <input type="file" name="myImage">
  <button>Add Tea</button>
</form>
</div>

<script src ="/js/cabinet.js"></script>